<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="1"
    tools:context=".HalamanSearch">
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/swipenya">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tbIndex"
            android:background="@color/colorPrimary"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            app:title="Jadwal Majelis"
            />
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="4dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                              <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardElevation="4dp"
                    android:layout_margin="5dp"
                    android:id="@+id/cdJakpus">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:textColor="@color/colorPrimary"
                            android:id="@+id/txtJakpus"
                            android:text="Jakarta Pusat"/>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            android:id="@+id/nullJakpus"
                            android:visibility="gone"

                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btnsecondary">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:padding="2dp"
                                android:textColor="@color/colorPrimary"
                                android:text="Tidak ada jadwal"
                                />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            app:cardBackgroundColor="@color/colorPrimary"
                            android:id="@+id/cdJadwalJakpus"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:textStyle="bold"
                                    android:paddingLeft="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:textColor="@color/cardview_light_background"
                                    android:id="@+id/txtJmlJakpus"/>

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardElevation="4dp"
                    android:layout_margin="5dp"
                    android:id="@+id/cdJakut">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:textColor="@color/colorPrimary"
                            android:id="@+id/txtJakut"
                            android:text="Jakarta Utara"/>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            android:id="@+id/nullJakut"
                            android:visibility="gone"

                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btnsecondary">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:padding="2dp"
                                android:textColor="@color/colorPrimary"
                                android:text="Tidak ada jadwal"
                                />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            app:cardBackgroundColor="@color/colorPrimary"
                            android:id="@+id/cdJadwalJakut"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:textStyle="bold"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="10dp"
                                    android:layout_gravity="center"
                                    android:textColor="@color/cardview_light_background"
                                    android:id="@+id/txtJmlJakut"/>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardElevation="4dp"
                    android:layout_margin="5dp"
                    android:id="@+id/cdJaktim">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:textColor="@color/colorPrimary"
                            android:id="@+id/txtJaktim"
                            android:text="Jakarta Timur"/>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            android:id="@+id/nullJaktim"
                            android:visibility="gone"

                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btnsecondary">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:padding="2dp"
                                android:textColor="@color/colorPrimary"
                                android:text="Tidak ada jadwal"
                                />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            app:cardBackgroundColor="@color/colorPrimary"
                            android:id="@+id/cdJadwalJaktim"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:textStyle="bold"
                                    android:id="@+id/txtJmlJaktim"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:paddingLeft="10dp"
                                    android:textColor="@color/cardview_light_background" />


                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardElevation="4dp"
                    android:layout_margin="5dp"
                    android:id="@+id/cdJakbar">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:textColor="@color/colorPrimary"
                            android:id="@+id/txtJakbar"
                            android:text="Jakarta Barat"/>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            android:id="@+id/nullJakbar"
                            android:visibility="gone"

                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btnsecondary">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:padding="2dp"
                                android:textColor="@color/colorPrimary"
                                android:text="Tidak ada jadwal"
                                />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            app:cardBackgroundColor="@color/colorPrimary"
                            android:id="@+id/cdJadwalJakbar"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:textStyle="bold"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:paddingLeft="10dp"
                                    android:textColor="@color/cardview_light_background"
                                    android:id="@+id/txtJmlJakbar"/>

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardElevation="4dp"
                    android:layout_margin="5dp"
                    android:id="@+id/cdJaksel">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:textColor="@color/colorPrimary"
                            android:id="@+id/txtJaksel"
                            android:text="Jakarta Selatan"/>

                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            android:id="@+id/nullJaksel"
                            android:visibility="gone"

                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btnsecondary">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textStyle="bold"
                                    android:padding="2dp"
                                    android:textColor="@color/colorPrimary"
                                    android:text="Tidak ada jadwal"
                                    />
                            </LinearLayout>


                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="115dp"
                            android:layout_height="25dp"
                            app:cardBackgroundColor="@color/colorPrimary"
                            android:id="@+id/cdJadwalJaksel"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="3dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:textStyle="bold"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="10dp"
                                    android:layout_gravity="center"
                                    android:textColor="@color/cardview_light_background"
                                    android:id="@+id/txtJmlJaksel"/>

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

            </LinearLayout>
        </androidx.cardview.widget.CardView>





</LinearLayout>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</androidx.constraintlayout.widget.ConstraintLayout>